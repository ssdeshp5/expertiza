<h1>Add Team</h1>

<%= form_tag url_for(:action => 'add_team'), method: "put" do %>
    <%= render :partial => 'topic' %>
    <%= hidden_field_tag('id', params[:id]) %>
    <%= hidden_field_tag('assignment_id', @topic.assignment_id) %>
    <%= render :partial => 'team' %>
    <%= hidden_field_tag('id', params[:id]) %>
    <%= hidden_field_tag('team_id', @team.team_id) %>
    <%= submit_tag "Add Team"%>
<% end %>


